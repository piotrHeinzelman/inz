echo '! conda install keras'

echo '! conda create -n tf10 python=3.10'
echo '! conda install conda-forge::tensorflow-gpu=2.16.1'
echo '! conda update -n base -c defaults conda'

echo '! conda init'
echo '! conda activate tf'

echo 'conda create --name tf-gpu tensorflow-gpu cudatoolkit=12.8'
echo 'conda activate tf-gpu'


echo 'for a in /sys/bus/pci/devices/*; do echo 0 | sudo tee -a $a/numa_node; done'


conda deactivate

python3 Python/python_pytorch.py >> log/python_pytorch.log

conda activate tf

python3 Python/python_tensorflow.py >> log/python_tensorflow.log
